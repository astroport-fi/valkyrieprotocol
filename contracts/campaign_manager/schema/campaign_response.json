{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CampaignResponse",
  "type": "object",
  "required": [
    "address",
    "code_id",
    "created_height",
    "creator"
  ],
  "properties": {
    "address": {
      "type": "string"
    },
    "code_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "created_height": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "creator": {
      "type": "string"
    }
  }
}
